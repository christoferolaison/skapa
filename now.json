{
  "version": 2,
  "name": "skapa",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/$1.html"
    },
    {
      "src": "/",
      "dest": "/web/index.html"
    }
  ],
  "alias": ["skapa"],
  "github": {
    "autoAlias": false
  }
}
